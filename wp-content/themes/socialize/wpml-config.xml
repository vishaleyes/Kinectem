<wpml-config> 

	<custom-fields>
		<custom-field action="translate">post_page_header_text</custom-field>
		<custom-field action="translate">post_custom_title</custom-field>
		<custom-field action="translate">post_subtitle</custom-field>
		<custom-field action="translate">page_page_header_text</custom-field>
		<custom-field action="translate">page_custom_title</custom-field>
		<custom-field action="translate">page_subtitle</custom-field>
		<custom-field action="translate">blog_template_page_header_text
		<custom-field action="translate">blog_template_custom_title</custom-field>
		<custom-field action="translate">blog_template_subtitle</custom-field>
		<custom-field action="translate">portfolio_template_page_header_text
		<custom-field action="translate">portfolio_template_custom_title</custom-field>
		<custom-field action="translate">portfolio_template_subtitle</custom-field>
