		<custom-field action="translate">portfolio_item_custom_title</custom-field>
		<custom-field action="translate">portfolio_item_subtitle</custom-field>
		<custom-field action="translate">bbpress_page_header_text</custom-field>
		<custom-field action="translate">events_post_page_header_text</custom-field>
		<custom-field action="translate">slide_caption_title</custom-field>
